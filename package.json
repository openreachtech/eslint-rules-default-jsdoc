{
  "name": "@openreachtech/eslint-rules-default",
  "version": "1.0.0",
  "description": "A showcasing package all the rules of ESLint.",
  "main": "index.js",
  "exports": {
    ".": {
      "import": "./index.mjs",
      "require": "./index.js",
      "node": "./index.js",
      "default": "./index.js"
    }
  },
